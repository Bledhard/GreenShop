{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:65509",
      "sslPort": 44391
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "development": {
      "commandName": "IISExpress",
      "launchUrl": "api/categories",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "development"
      }
    },
    "staging": {
      "commandName": "Project",
      "launchUrl": "api/categories",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "staging"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "production": {
      "commandName": "Project",
      "launchUrl": "api/categories",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "production"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/categories",
      "httpPort": 65510,
      "useSSL": true,
      "sslPort": 44392
    }
  }
}